{
  "name": "fctest",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "14.17.4"
  },
  "scripts": {
    "start_js": "tsc --p tsconfig.json && yarn run serve",
    "start": "NODE_ENV=development ts-node src/server.ts",
    "serve": "node build/server.js",
    "build": "yarn run clean && yarn run build-ts",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon -r dotenv/config build/server.js",
    "clean": "rimraf ./build",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "eslint": "eslint . --ext .js,.ts",
    "oldTest": "jest --forceExit --detectOpenHandles --coverage --verbose",
    "test": "NODE_ENV=testing jest  --forceExit --runInBand --verbose src/services/CacheService/CacheService.test.ts"
  },
  "dependencies": {
    "@types/helmet": "^4.0.0",
    "@types/uuid": "^8.3.4",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.1",
    "boom": "^7.3.0",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.2.0",
    "jest": "^27.4.7",
    "joi": "^17.5.0",
    "mongo-unit": "^2.0.1",
    "mongodb-memory-server": "^8.1.0",
    "mongoose": "5.11.10",
    "morgan": "^1.10.0",
    "redis": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/register": "^7.16.7",
    "@types/bluebird": "^3.5.36",
    "@types/body-parser": "^1.19.2",
    "@types/boom": "^7.3.1",
    "@types/chai": "^4.3.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.6",
    "@types/redis": "^2.8.21",
    "@types/supertest": "^2.0.11",
    "chai": "^4.3.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest-cli": "^27.4.7",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2"
  }
}
